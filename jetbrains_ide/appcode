#!/bin/bash
#Generated by JetBrains Toolbox 1.23.11731 at 2022-04-15T14:11:45.705108

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/uzaaft/Library/Application Support/JetBrains/Toolbox/apps/AppCode/ch-0/213.7172.21/AppCode.app/Contents/MacOS/appcode" $wait --args "${ideargs[@]}"