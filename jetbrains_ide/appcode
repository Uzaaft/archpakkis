#!/bin/bash
#Generated by JetBrains Toolbox 1.25.12627 at 2022-09-02T17:09:29.276799

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/uzaaft/Library/Application Support/JetBrains/Toolbox/apps/AppCode/ch-0/222.3739.57/AppCode.app/Contents/MacOS/appcode" $wait --args "${ideargs[@]}"