#!/bin/bash
#Generated by JetBrains Toolbox 1.26.5.13419 at 2022-11-04T11:42:44.145802

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/uzaaft/Library/Application Support/JetBrains/Toolbox/apps/AppCode/ch-0/222.4345.34/AppCode.app/Contents/MacOS/appcode" $wait --args "${ideargs[@]}"