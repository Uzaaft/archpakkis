#!/bin/bash
#Generated by JetBrains Toolbox 1.25.12424 at 2022-07-08T18:49:57.193557

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/uzaaft/Library/Application Support/JetBrains/Toolbox/apps/PyCharm-P/ch-0/222.3345.3/PyCharm 2022.2 EAP.app/Contents/MacOS/pycharm" $wait --args "${ideargs[@]}"