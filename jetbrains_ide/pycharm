#!/bin/bash
#Generated by JetBrains Toolbox 1.24.12080 at 2022-06-02T13:13:10.924083

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/uzaaft/Library/Application Support/JetBrains/Toolbox/apps/PyCharm-P/ch-0/221.5787.24/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${ideargs[@]}"