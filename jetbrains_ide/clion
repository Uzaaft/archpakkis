#!/bin/bash
#Generated by JetBrains Toolbox 1.24.12080 at 2022-06-23T08:30:34.534937

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/uzaaft/Library/Application Support/JetBrains/Toolbox/apps/CLion/ch-0/222.3048.7/CLion 2022.2 EAP.app/Contents/MacOS/clion" $wait --args "${ideargs[@]}"